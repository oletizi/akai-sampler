{
  "name": "@oletizi/translate",
  "version": "1.0.0",
  "description": "Sampler format translation utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "nyc mocha",
    "prepublish": "tsc"
  },
  "nyc": {
    "extension": [
      ".ts",
